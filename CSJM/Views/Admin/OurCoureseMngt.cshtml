@{
    ViewBag.Title = "OurCoureseMngt";
    Layout = "~/Views/Shared/AdminPageLayout.cshtml";
}

<div class="row">
    <div class="col-lg-3 col-md-3  col-sm-4 col-xs-12 col-3"></div>
    <div class="col-lg-6 col-md-6 col-sm-4 col-xs-12 col-6 card1" style="padding:0 50px 0 50px;">
        @Html.ActionLink("Delete Courses","../Admin/DeleteCourses" , "DeleteCourses")
         <center> <i class="fa fa-graduation-cap" style="font-size:36px; color:#ffffff;"></i><font style="font-size:30px;">  <strong>Our Courese Management</strong></font></center>
        <br />
        <form action="../Admin/OurCoureseMngt" method="post" enctype="multipart/form-data">
            <label>Coures Name:</label>
            <select class="form-control" name="txtOurCourse">
                <option><---  Select Option   ---></option>
                <option>Electronics Engineering</option>
                <option>Computer Science & Engineering</option>
                <option>Information of Technology</option>
            </select><br />
            <label>Coures Related Image:</label>
            <input type="file" name="txtfile" class="form-control" /><br />
            <label>Stranth of Studanet:</label>
            <input type="text" name="txtIntake" class="form-control" placeholder="Enter the Intake" /><br />
            <label>Duration of Year:</label>
            <input type="text" name="txtDuration" class="form-control" placeholder="Enter the Duration" /><br />
            <label>@ViewBag.result</label>
            <input type="submit" value="Save" class="btn btn-success form-control" />
          
            <br /><br />

        </form>


    </div>
    <div class="col-lg-3 col-md-3  col-sm-4 col-xs-12 col-3"></div>
    
</div>

